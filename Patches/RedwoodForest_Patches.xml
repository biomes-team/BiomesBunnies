<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biome Expansion: Redwood forest</li>
		</mods>
<!-- like boreal forest -->
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd"> 
					<xpath>Defs/BiomeDef[defName="RedwoodForest"]/wildAnimals</xpath>
					<value>
						<Bunnies_ChocolateRabbit>0.15</Bunnies_ChocolateRabbit>
						<Bunnies_DustBunny>0.6</Bunnies_DustBunny>
						<Bunnies_Hoptimus>0.15</Bunnies_Hoptimus>
						<Bunnies_HoptimusPrime>0.1</Bunnies_HoptimusPrime>
					</value>
				</li>
					
			</operations>
		</match>
	</Operation>


</Patch>

