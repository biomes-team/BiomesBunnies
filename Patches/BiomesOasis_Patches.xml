<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biomes! Oasis</li>
		</mods>
<!-- like boreal forest -->
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd"> 
					<xpath>Defs/BiomeDef[defName="BMT_ChromaticOasis"]/wildAnimals</xpath>
					<value>
						<Bunnies_ChocolateRabbit>0.1</Bunnies_ChocolateRabbit>
						<Bunnies_EmpireRabbit>0.05</Bunnies_EmpireRabbit>
						<Bunnies_Microbunny>0.15</Bunnies_Microbunny>
						<Bunnies_Wolpertinger>0.05</Bunnies_Wolpertinger>
					</value>
				</li>
					
			</operations>
		</match>
	</Operation>


</Patch>

