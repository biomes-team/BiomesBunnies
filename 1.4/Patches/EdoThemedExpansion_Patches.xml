<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Edo Themed Expansion</li>
		</mods>
<!-- like temperate forest -->
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd"> 
					<xpath>Defs/BiomeDef[defName="HSBambooForest"]/wildAnimals</xpath>
					<value>
						<Bunnies_ChocolateRabbit>0.05</Bunnies_ChocolateRabbit>
						<Bunnies_DustBunny>0.2</Bunnies_DustBunny>
						<Bunnies_Hoptimus>0.1</Bunnies_Hoptimus>
						<Bunnies_HoptimusPrime>0.02</Bunnies_HoptimusPrime>
						<Bunnies_EmpireRabbit>0.2</Bunnies_EmpireRabbit>
						<Bunnies_Rabbuck>0.2</Bunnies_Rabbuck>
						<Bunnies_Microbunny>0.2</Bunnies_Microbunny>
	
					</value>
				</li>
					
			</operations>
		</match>
	</Operation>


</Patch>

